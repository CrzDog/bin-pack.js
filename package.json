{
  "name": "bin-pack.js",
  "version": "1.0.1",
  "description": "2d rectangular bin packing in javascript",
  "main": "dist/bin-pack.js",
  "scripts": {
    "build:dev": "rollup -c ./script/rollup.config.js",
    "build:min": "uglifyjs ./dist/bin-pack.dev.js --mangle --source-map ./dist/bin-pack.min.js.map -o ./dist/bin-pack.min.js",
    "release": "npm run build:dev && npm run build:min",
    "start": "electron",
    "server": "preview -p 8002 -e examples",
    "dev": "rollup -w -c ./script/rollup.config.js",
    "test": "npm run build:dev && tap test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gamedev-js/bin-pack.js.git"
  },
  "keywords": [
    "bin-pack",
    "algorithm",
    "texture"
  ],
  "author": "jwu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gamedev-js/bin-pack.js/issues"
  },
  "homepage": "https://github.com/gamedev-js/bin-pack.js/issues",
  "dependencies": {},
  "devDependencies": {
    "fs-jetpack": "^0.13.0",
    "preview-server": "^1.0.7",
    "rollup": "^0.41.4",
    "rollup-watch": "^3.2.2",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony"
  }
}
